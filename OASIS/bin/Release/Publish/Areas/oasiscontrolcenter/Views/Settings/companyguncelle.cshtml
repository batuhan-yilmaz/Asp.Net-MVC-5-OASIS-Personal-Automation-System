@model OASIS.Models.Entity.company

@{
    ViewBag.Title = "Firma Güncelle";
    Layout = "~/Areas/oasiscontrolcenter/Views/Shared/_Layout.cshtml";
}

<div class="content-wrapper">
    <div class="card-body">
        <h2>Firma Güncelle</h2>
        @using (Html.BeginForm("cGuncelle", "Settings", FormMethod.Post))
        {
    <table id="example1" class="table table-bordered table-striped">
        <tr>
            <td>
                <p>Firma</p>
                @Html.TextBoxFor(m => m.company1, new {oninput = "customtext(this.id);", @class = "form-control", @required = "required" })
                @Html.HiddenFor(m => m.companyid, new { @class = "form-control", @required = "required" })
            </td>
        </tr>
        <tr>
            <td><button type="submit" class="fa fa-floppy-o btn btn-success" /> Kaydet</td>
        </tr>

    </table>

        }

    </div>
</div>

<script src="~/Content/Admin/plugins/custom/jquery-ui.js"></script>
<script src="~/Content/Admin/plugins/custom/oasiscustom.js"></script>




